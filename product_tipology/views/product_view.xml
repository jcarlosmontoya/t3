<odoo>
    <data>
        <!--Agregar campo tipology antes del campo type en product-->
        <record id="product_template_form_view_tipology" model="ir.ui.view">
            <field name="name">product.template.form.view.tipology</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_form_view"/>
            <field name="arch" type="xml">
                <field name="type" position="before">
                    <field name="tipology"/>
                </field>
            </field>
        </record>
    </data>
</odoo>